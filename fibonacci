#include <iostream>

using namespace std;

int main() {

	clock_t start, end;

	/* Recording the starting clock tick.*/
	start = clock();

	int a{}, b{ 1 }, c{};

	cout << a << " " << b << " ";

	for (int i = 0; i < 40; i ++){
		c = b;
		b = a + b;
		a = c;

		cout << b << " ";
		
	}

	cout << endl;

	end = clock();

	// Calculating total time taken by the program.
	double time_taken = double(end - start) / double(CLOCKS_PER_SEC);
	cout << "Time taken by program is : " << fixed
		<< time_taken;
	cout << " sec " << endl;




}
